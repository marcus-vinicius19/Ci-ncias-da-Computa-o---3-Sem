package a1303;public class Slist { 	int size;	Node head;			public Slist() {		this.size = 0;		this.head = null;	}	public void insereNodeInicio(int item) {		Node x = new Node(item);		if (this.size == 0 ) {		this.head = x;		this.size++;		}		else {		x.next = this.head ;		this.head = x;		this.size++;		}	}		public void insereNodeFim(int item) {		Node x = new Node(item);		if (this.size == 0) {		this.head = x;		this.size++;		}		else {		int contador = 1;		Node trab = this.head;		while (contador < this.size) {		trab = trab.next ;		contador++;		}		trab.next = x;		this.size++;		}			}		public void imprimeLista() {		if (this.size == 0) {			System.out.println("Lista vazia");			}			else {				int contador = 1;				Node trab = this.head;				while (contador <= this.size) {				System.out.print (" " + trab.item);				trab = trab.next;				contador++;					}			}}}=++++++++++++++++++++++++++++++++++++++++++++++

package a1303;public class Node{		int item;	Node next;			public Node() {		this.item = 0;		this.next = null;	}		public Node(int item) {		this.item = item;		this.next = null;	}}
