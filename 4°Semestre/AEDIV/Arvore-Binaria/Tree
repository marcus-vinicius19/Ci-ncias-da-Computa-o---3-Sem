package binary_tree;

public class Tree {
	
	int size;
	Node head;
	
	public Tree(int item) {
		this.size = 1;
		this.head = new Node(item);
	}
	
	public void Insert(int item) {
		this.size++;
		this.head.Insert(item);
	}
	
	public void Insert_descending(int item) {
		this.size++;
		this.head.Insert_descending(item);
	}
	
	public void Print_in_order() {
		this.head.Print_in_order(this.head);
	}
	
	public void Print_in_pre_order() {
		this.head.Print_in_pre_order(this.head);
	}
	
	public void Print_in_post_order() {
		this.head.Print_in_post_order(this.head);
	}
}
